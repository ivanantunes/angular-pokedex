{
  "name": "angular-pokedex",
  "version": "0.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run indexToDeploy && ng build && npm run indexToDev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "indexToDeploy": "mv src/index.html src/index_dev.html && mv src/index_deploy.html src/index.html",
    "indexToDev": "mv src/index.html src/index_deploy.html && mv src/index_dev.html src/index.html",
    "postversion": "git push && git push --tags",
    "deploy": "npm run build && npx angular-cli-ghpages --dir=dist/angular-pokedex"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/cdk": "^17.1.1",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/material": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@swimlane/ngx-charts": "^20.5.0",
    "bootstrap": "^5.3.2",
    "dexie": "^3.2.4",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular/cli": "~17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.10",
    "@types/d3-shape": "^3.1.6",
    "@types/jasmine": "~4.3.0",
    "angular-cli-ghpages": "^1.0.7",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.3"
  }
}
